              <h3 id="photos_count" style="text-align:left;"><%= pluralize(@user.albums.size, "Photo")%></h3>
                <div id="uploads">
                    <%= render :partial => @user.albums %>
                </div>

                <!-- Upload picture -->
  					    <%= form_for [:user,Album.new(:user_id => @user.id)], :html => {:multipart => true} do |f| %> 
                   <table width="100%">
          					<%= f.hidden_field :user_id, :value => @user.id %>
          					<tr>
          						  <td align="left" width="54%" style="vertical-align: top !important;"><strong>Upload more pictures for your highlight reel!</strong></td>
        						    <td align="left"><%= f.file_field :photo %></td>
        						</tr>
                   </table>
                <% end %>
                <!-- End Upload picture -->
                

  						  <% form_for :users do |f| %>
  						    <table width="100%">
  						    <%= f.hidden_field :user_id, :value => @user.id %>
  						    <tr>
  						        <td align="left" width="50%" style="vertical-align: top !important;"><strong>Summary ( a little bit about yourself )</strong></td>
  						        <td align="left"><%= f.text_area :title, :rows => 3, :cols => 36 %></td>
            			</tr>
  						          
                  <tr>
              				<td align="left" width="50%" style="vertical-align: top !important;"><strong> What are you looking for in your "best supporting actor/actress?</strong></td>
  						        <td align="left"><%= f.text_area :supporting_actor, :rows => 3, :cols => 36 %></td>
            			</tr>
            			<tr>
            			    <td colspan="2">&nbsp;</td>
            			</tr>
                          
                  <tr>
              				<td colspan="2">    
                      <div class="formArea_next">
  						        <p align="right"  class="cont1_next" style="width:550px;"> 
  						          <%= link_to "Skip for now and start browsing", {:controller=>"dashboard", :action=>'index'} %>
  						          <span style="padding-left:25px;">
  						            <%= f.submit "Next", :class=>'button' %>
  						          </span>
  						        </p>
  						        </div>
  						        </td>
  						    </tr>
  						    </table>
                 <% end %>
              
              
				</td>
			</tr>
		</table>
		  </div>
    </div>
</div> 

 


