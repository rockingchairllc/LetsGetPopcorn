 <%= render :partial => "uploadify" %> 

 <div id="dialog" style="display:none;">
	<div class="div_inner">
		<div class="heading" ><strong>You're almost there!</strong></div> 
      <div style="padding-left:0px;">
		  <table width="95%" border="0" cellspacing="2" cellpadding="2">
